<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:theme="@style/Theme.AppCompat.NoActionBar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoinicio"
    tools:context=".MainActivity" >

    <TextView
        android:id="@+id/textView"
        android:layout_width="989dp"
        android:layout_height="269dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="744dp"
        android:fontFamily="@font/quantico_bold"
        android:text="@string/app_name"
        android:textColor="@color/colorBotones"
        android:textSize="200sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="958dp"
        android:layout_height="255dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="744dp"
        android:fontFamily="@font/quantico_bold"
        android:text="@string/app_name"
        android:textColor="@color/colorTexto"
        android:textSize="200sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="963dp"
        android:layout_height="208dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="1112dp"
        android:background="@color/colorBotones"
        android:fontFamily="@font/quantico_bold"
        android:text="@string/botoninicio"
        android:textAllCaps="false"
        android:textColor="@color/colorTexto"
        android:textSize="100sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>